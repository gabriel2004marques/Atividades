<html>

     <head>

        <title>Números4</title>

        <script>
            
            var N1 = parseFloat(prompt("Digite um número "))
            var N2 = parseFloat(prompt("Digite outro número "))
            var N3 = parseFloat(prompt("Digite outro número"))
            var resultadoN1N2 = N1 + N2
            var resultadoN1N3 = N1 + N3 
            var resultadoN2N3 = N2 + N3

            if (N1 > N2) {
                alert("O resultado da soma entre "+N1+ " e "+N3+" é : "+resultadoN1N3+". Fim")
            } 
            if (N3 > N1) {
                alert("O resultado da soma entre "+N3+ " e "+N2+" é : "+resultadoN2N3+". Fim")
            } else if(N2 > N3) {
                alert("O resultado da soma entre "+N1+" e "+N2+" é : "+resultadoN1N2+". Fim")
            }
          
          

        </script>

    </head>

<body>
</body>

</html>
